{
    "workbench.colorTheme": "Low Eye Strain Neon",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontSize": 20,
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "'Monaspace Radon', monospace",
    "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga'",
    "editor.wordWrap": "wordWrapColumn",
    "editor.codeActionsOnSave": {

    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "phpserver.phpConfigPath": "/etc/php/php.ini 4",
    "phpserver.phpPath": "/usr/bin/php",
    "php.validate.executablePath": "/usr/bin/php",
    "tabnine.experimentalAutoImports": true,
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "symbols",
    "terminal.integrated.env.linux": {},
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": true
}